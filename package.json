{
  "name": "chatsquad",
  "version": "1.0.0",
  "description": "Chat server for dialogflow",
  "main": "bin/index.js",
  "author": "Pujit Mehrotra",
  "license": "MIT",
  "private": true,
  "scripts": {
    "compile": "tsc",
    "start": "tsc && node -r esm -r module-alias/register bin/index.js",
    "start:dev": "cross-env DEBUG=* ts-node-dev --respawn -r tsconfig-paths/register src/index.ts",
    "start:debug:prod": "cross-env NODE_ENV=production npm run start:dev"
  },
  "dependencies": {
    "@flight-squad/admin": "0.1.5",
    "@google-cloud/storage": "^4.1.2",
    "@types/debug": "^4.1.5",
    "actions-on-google": "^2.12.0",
    "axios": "^0.19.0",
    "debug": "^4.1.1",
    "dialogflow": "^0.14.1",
    "dialogflow-fulfillment": "^0.6.1",
    "dotenv": "^8.2.0",
    "envkey": "^1.2.7",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "heroku-logger": "^0.3.3",
    "messaging-api-messenger": "^0.8.2",
    "module-alias": "^2.2.2",
    "openflights-cached": "^1.1.6",
    "path": "^0.12.7",
    "typescript": "^3.7.2",
    "uuid": "^3.3.3"
  },
  "_moduleAliases": {
    "config": "bin/config",
    "api": "bin/api",
    "actions": "bin/actions",
    "intents": "bin/intents",
    "modules": "bin/modules",
    "data": "bin/data"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0"
  }
}
